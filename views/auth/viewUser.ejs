<div
  class="d-flex justify-content-between flex-wrap flex-md nowrap align-item-center pt-3 pb-2 border-bottom"
>
  <h1 class="h2">Users Name: <%=currentUser.name%></h1>
  <div class="btn-toolbar mb-2 mb-md 0">
    <div class="btn-group me-2"></div>
  </div>
</div>
<div class="col py-3">
  <div class="row">
    <div class="col">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="/">Dashboard</a>
          </li>
          <li class="breadcrumb-item">
            <a href="/auth/usersInfo">Users Information</a>
          </li>
          <li class="breadcrumb-item active"><%=currentUser.name%></li>
        </ol>
      </nav>
    </div>
    <div class="col text-end fw-lighter">
      <b>Last Update <%= new Date(currentUser.updatedAt).toUTCString() %></b>
    </div>
  </div>
</div>
<ul class="list-group">
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>Name </b></div>
      <div class="col"><%=currentUser.name%></div>
    </div>
  </li>
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>Email </b></div>
      <div class="col"><%=currentUser.email%></div>
    </div>
  </li>
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>Contact Number </b></div>
      <div class="col"><%=currentUser.tel%></div>
    </div>
  </li>
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>User Name </b></div>
      <div class="col"><%=currentUser.username%></div>
    </div>
  </li>
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>Date Created </b></div>
      <div class="col"><%=currentUser.createdAt%></div>
    </div>
  </li>
  <li class="list-group-item">
    <div class="row">
      <div class="col" style="max-width: 140px"><b>Date Modified </b></div>
      <div class="col"><%=currentUser.updatedAt%></div>
    </div>
  </li>
</ul>
